{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://fradotovic-001-site1.jtempurl.com/api/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}
